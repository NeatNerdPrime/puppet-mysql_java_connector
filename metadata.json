{
  "name": "puppet-mysql_java_connector",
  "version": "5.0.0",
  "author": "Vox Pupuli",
  "summary": "This module manages the upstream MySQL Java Connector (Connector/J).",
  "license": "MIT",
  "source": "https://github.com/voxpupuli/puppet-mysql_java_connector",
  "project_page": "https://github.com/voxpupuli/puppet-mysql_java_connector",
  "issues_url": "https://github.com/voxpupuli/puppet-mysql_java_connector/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.13.1 < 9.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 1.0.1 < 7.0.0"
    }
  ],
